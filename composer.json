{
    "name": "juliastrelkova/project-lvl1-s252",
    "type": "library",
    "description": "My version of Hexlet brain-games project",
    "authors": [
        {
            "name": "Julia Strelkova",
            "email": "strelkovajuli@gmail.com"
        }
    ],
    "require": {
        "wp-cli/php-cli-tools": "^0.11.8"
    },
    "bin": [
        "bin/brain-games",
        "bin/brain-even",
        "bin/brain-calc",
        "bin/brain-gcd"
    ],
    "autoload": {
        "psr-4": {
            "BrainGames\\": "src"
        },
        "files": [
            "src/Cli.php",
            "src/GamesConfigurations.php",
            "src/Games/brain-even.php",
            "src/Games/brain-calc.php",
            "src/Games/brain-gcd.php"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2"
    },
    "scripts": {
        "phpcs": "phpcs"
    },
    "config": {
        "bin-dir": "vendor/bin"
    }
}
